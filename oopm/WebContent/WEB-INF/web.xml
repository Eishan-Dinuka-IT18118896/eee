  </servlet>
  <servlet>
    <servlet-name>DeleteReserVehicalServlet</servlet-name>
    <servlet-class>servlet.DeleteReserVehicalServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>GetReserVehicalServlet</servlet-name>
