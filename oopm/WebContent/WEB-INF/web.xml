    <servlet-name>AddReserVehicalServlet</servlet-name>
    <url-pattern>/AddReserVehicalServlet</url-pattern>
  </servlet-mapping>
  
  <servlet-mapping>
    <servlet-name>UpdateReserVehicalServlet</servlet-name>
    <url-pattern>/UpdateReserVehicalServlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>DeleteReserVehicalServlet</servlet-name>
    <url-pattern>/DeleteReserVehicalServlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>GetReserVehicalServlet</servlet-name>
    <url-pattern>/GetReserVehicalServlet</url-pattern>
  </servlet-mapping>
   <servlet-mapping>
    <servlet-name>ListReserVehicalServlet</servlet-name>
    <url-pattern>/ListReserVehicalServlet</url-pattern>
  </servlet-mapping> 
 
 	<servlet-mapping>
		<servlet-name>LoginServlet</servlet-name>
		<url-pattern>/LoginServlet</url-pattern>
	</servlet-mapping>
	 
  <welcome-file-list>

